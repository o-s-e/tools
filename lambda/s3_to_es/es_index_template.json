{
  "template": "s3_access_log-*",
  "order": 1,
  "settings": {
    "index": {
      "refresh_interval": "1s"
    }
  },
  "mappings": {
    "S3": {
      "_timestamp": {
        "enabled": true,
        "stored": true,
        "path": "@timestamp"
      },
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "bucket": {
          "type": "string",
          "index": "not_analysed"
        },
        "client_ip": {
          "type": "ip"
        },
        "owner_id": {
          "type": "string",
          "index": "not_analysed"
        },
        "requester": {
          "type": "string",
          "index": "not_analysed"
        },
        "request_id": {
          "type": "string",
          "index": "not_analysed"
        },
        "request_uri": {
          "type": "string",
          "index": "not_analysed"
        },
        "referrer": {
          "type": "string",
          "index": "not_analysed"
        },
        "error_code": {
          "type": "string",
          "index": "not_analysed"
        },
        "http_status_code": {
          "type": "string",
          "index": "not_analysed"
        },
        "operation": {
          "type": "string",
          "index": "not_analysed"
        },
        "user_agent": {
          "type": "string",
          "index": "not_analysed"
        },
        "version_id": {
          "type": "string",
          "index": "not_analysed"
        },
        "key": {
          "type": "string",
          "index": "not_analysed"
        },
        "total_time": {
          "type": "long"
        },
        "turn_around_time": {
          "type": "long"
        },
        "object_size": {
          "type": "byte"
        },
        "bytes_send": {
          "type": "byte"
        }
      }
    }
  }
}